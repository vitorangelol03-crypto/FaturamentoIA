{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "regions": ["gru1"],
  "functions": {
    "api/extract-invoice.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/sefaz-monitor.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ]
}
